&inputpars

!-- misc
in_nomp = 1
in_io_grabstdout = f
in_puretran = t
in_noreadstruct = t
in_noeos = t

!-- time step
in_tsp_tfirst = 0
in_tsp_tlast = 1d-2
in_tsp_nt = 100
in_tsp_itrestart = 1
in_alpha = 1d0
in_ismodimc = f

!-- grid settings
in_grd_igeom = 11
in_ndim = 32, 1, 1
in_isvelocity = f
in_novolsrc = f
in_str_lx = 1d0
in_str_ly = 1d0
in_str_lz = 1d0
in_str_totmass = 1d0
