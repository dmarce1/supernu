!-- group absorption opacity structure type
in_opacanaltype = 'grey'

!-- group settings
in_grp_ng = 1

!-- particle numbers
in_src_n2s  = 16
in_prt_n2max = 23

!-- imc tally method
in_trn_isimcanlog = f
in_trn_isddmcanlog = f
!-- ddmc mfp per cell
in_trn_tauddmc = 3d0
in_taulump = 3d0
in_trn_tauvtime = 'unif'

!-- opacity debug switches
in_nobbopac = t
in_nobfopac = t
in_noffopac = t
in_nothmson = t
in_noplanckweighting = t

